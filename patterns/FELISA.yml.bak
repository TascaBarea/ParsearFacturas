proveedor: "FELISA"
anchors:
  fecha:
    find: ["Fecha"]
    direction: "right"
regex: '''\1'
  numero_factura:
    find: ["Factura de cliente", "Nº"]
    direction: "below"
regex: '''\1'
  lineas:
    find: ["Código", "Descripción", "Total"]
    region: "lines"
rules:
  - NormalizarTextos
  - UnicaLineaPorProducto
  - RepartirPortes
descripcion:
  mode: "original"
iva_default: 10
grouping: lineas
portes:
  mode: "prorratear"
precedence:
  force_pattern_over_dictionary: false
