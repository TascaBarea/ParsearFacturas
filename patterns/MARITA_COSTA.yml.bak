proveedor: "MARITA COSTA"
anchors:
  fecha:
    find: ["Fecha"]
    direction: "right"
regex: '''\1'
  numero_factura:
    find: ["Nº FACTURA", "FACTURA"]
    direction: "right"
regex: '''\1'
  lineas:
    find: ["Descripción", "Precio", "Importe"]
    region: "lines"
rules:
  - NormalizarTextos
  - UnicaLineaPorProducto
  - RepartirPortes
descripcion:
  mode: "original"
iva_default: 10
grouping: lineas
portes:
  mode: "prorratear"
precedence:
  force_pattern_over_dictionary: false
